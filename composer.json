{
	"name": "codekandis/validators",
	"description": "`codekandis/validators` is a library providing converter interfaces and classes.",
	"keywords": [
		"php",
		"library",
		"validators",
		"interfaces",
		"classes"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/validators/issues",
		"docs": "https://github.com/codekandis/validators/blob/master/README.md",
		"source": "https://github.com/codekandis/validators/tree/master"
	},
	"require": {
		"php": "^7.4",
		"codekandis/converters": "^0",
		"codekandis/regular-expressions": "^0"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"codekandis/phpunit": "^3"
